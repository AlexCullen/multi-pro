<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <!--<import resource="applicationContext-dao.xml"/>-->
    <!--<import resource="applicationContext-trans.xml"/>-->
    <!--<import resource="applicationContext-service.xml"/>-->

    <dubbo:application name="dubbo-manager-service"/>

    <dubbo:registry protocol="zookeeper" address="192.168.121.142:2181" timeout="30000"/>

    <dubbo:protocol name="dubbo" port="28180"/>

    <dubbo:service interface="com.douby.manager.service.ItemService" ref="itemServiceImpl"/>
    <dubbo:service interface="com.douby.manager.service.ItemCastService" ref="itemCastServiceImpl"/>

</beans>